function ExecAttack(r1, r2, l1, l2, b1, b2, is_guard, blend_type, artsr1, artsr2)
    if c_HasActionRequest == FALSE then
        return FALSE
    elseif env(1001) <= 0 then
        return FALSE
    end
    local request = GetAttackRequest(is_guard)
    if request == ATTACK_REQUEST_INVALID then
        return FALSE
    end
    local style = c_Style
    local atk_hand = HAND_RIGHT
    local is_find_atk = TRUE
    local is_atk_auto_aim = TRUE
    
    -- Ring of Punishment - Critical Hit
    if env(1116, 112006) == TRUE then
        local rand = math.random(0, 100)
        -- Lucky Weapon
        if env(1116, 112010) == TRUE then
            if rand <= 10 then
                act(2002, 160601600)
            end
        -- Normal Weapon
        elseif env(1116, 112010) == FALSE then
            if rand <= 5 then
                act(2002, 160601600)
            end
        end
    end
    
    -- Snickering Top Hat - Critical Hit
    if env(1116, 112080) == TRUE then
        local rand = math.random(0, 100)
        -- Lucky Weapon
        if env(1116, 112010) == TRUE then
            if rand <= 10 then
                act(2002, 160402030)
            end
        -- Normal Weapon
        elseif env(1116, 112010) == FALSE then
            if rand <= 5 then
                act(2002, 160402030)
            end
        end
    end
    
