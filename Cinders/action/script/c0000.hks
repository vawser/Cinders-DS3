function UpdateBulletRingEffect(trigger_effect, bullet_effect)
    if env(1116, trigger_effect) == TRUE then
        if env(1116, 112170) ~= TRUE and env(1116, 112171) ~= TRUE and env(1116, 112172) ~= TRUE and env(1116, 112173) ~= TRUE and env(1116, 112174) ~= TRUE and env(1116, 112175) ~= TRUE then
            
            -- Randomise cooldownd duration
            local roll = math.random(0, 100)
            if roll >= 0 and roll < 10 then
                act(2002, 112170)
            elseif roll >= 10 and roll < 30 then
                act(2002, 112171)
            elseif roll >= 30 and roll < 50 then
                act(2002, 112172)
            elseif roll >= 50 and roll < 70 then
                act(2002, 112173)
            elseif roll >= 70 and roll < 90 then
                act(2002, 112174)
            elseif roll >= 90 then
                act(2002, 112175)
            end
            
            act(2002, bullet_effect) -- Effect
        end
    end
end

