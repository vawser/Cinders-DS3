            return FALSE
        end
        SetAttackHand(HAND_LEFT)
        magic_hand = HAND_LEFT
    elseif env(1106, ACTION_ARM_R2) == TRUE then -- ActionRequest
        if env(1106, ACTION_ARM_HEAVY_KICK) == TRUE then -- ActionRequest
            return FALSE
        elseif style == HAND_LEFT_BOTH then
            local sp_kind = env("装備武器特殊カテゴリ番号取得", HAND_LEFT) -- GetEquipWeaponSpecialCategoryNumber
            if sp_kind ~= 178 and sp_kind ~= 207 and sp_kind ~= 217 and sp_kind ~= 220 and sp_kind ~= 234 and sp_kind ~= 257 then
                -- Hybrid Spell Tool
                return FALSE
            elseif sp_kind == 257 and env(1116, 100580) == TRUE then -- GetSpecialEffectID
                -- Demon's Scar
                return FALSE
            end
            SetAttackHand(HAND_LEFT)
        else
            local sp_kind = env("装備武器特殊カテゴリ番号取得", HAND_RIGHT) -- GetEquipWeaponSpecialCategoryNumber
            if sp_kind ~= 178 and sp_kind ~= 207 and sp_kind ~= 217 and sp_kind ~= 220 and sp_kind ~= 234 and sp_kind ~= 257 then
                -- Hybrid Spell Tool
                return FALSE
            elseif sp_kind == 257 and env(1116, 100580) == TRUE then -- GetSpecialEffectID
