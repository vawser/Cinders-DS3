-- Fool's Sigil
function UpdateSigilEffect()
    local roll = math.random(0, 100)
    
    -- Has Fool's Sigil equipped
    if env(1116, 112120) == TRUE then
        if env(1116, 112121) ~= TRUE then -- Not applied effect in past 15 seconds
            act(2002, 112121) -- Cooldown
            
            -- Attack
            if roll >= 0 and roll < 12 then
                act(2002, 112122)
            end

            -- Absorption
            if roll >= 12 and roll < 24 then
                act(2002, 112123)
            end
            
            -- Stamina Recovery
            if roll >= 24 and roll < 36 then
                act(2002, 112124)
            end
            
            -- FP Regen
            if roll >= 36 and roll < 48 then
                act(2002, 112125)
            end
            
            -- Stamina/FP Consumption
            if roll >= 48 and roll < 60 then
                act(2002, 112126)
            end
            
            -- Immunity to Status
            if roll >= 60 and roll < 72 then
                act(2002, 112127)
            end
            
            -- No Estus
            if roll >= 72 and roll < 84 then
                act(2002, 112128)
            end
            
            -- Negative Absorption
            if roll >= 84 and roll < 96 then
                act(2002, 112129)
            end
            
            -- Nothing
        end
    end
end

