                    end
                    return TRUE
                end
            elseif env(234) == FALSE and MoveStartonCancelTiming(Event_Move, FALSE) == TRUE then -- IsPrecisionShoot
                return TRUE
            end
        end
        if lower_state == LOWER_END_TURN then
            ExecEventHalfBlendNoReset(event, LOWER)
            return TRUE
        end
    end
    return FALSE
end

